{
  "name": "alex-bridge-astro",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "bun@1.3.0",
  "scripts": {
    "sass:watch": "sass --watch src/styles:src/styles",
    "sass:build": "sass src/styles:src/styles --no-source-map",
    "dev": "concurrently \"bun run sass:watch\" \"bunx astro dev\"",
    "build": "bun run sass:build && bunx astro build",
    "preview": "bunx astro preview",
    "astro": "bunx astro",
    "format": "bunx prettier --write .",
    "lint": "bunx eslint .",
    "lint:fix": "bunx eslint --fix ."
  },
  "dependencies": {
    "@fontsource/space-grotesk": "^5.2.10",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/list": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "decap-cms-app": "^3.8.4",
    "globals": "^16.4.0",
    "node-ical": "^0.22.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "remixicon": "^4.7.0"
  },
  "devDependencies": {
    "@astrojs/react": "^4.4.0",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/vercel": "^8.2.11",
    "@eslint/js": "^9.38.0",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "astro": "^5.14.7",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "eslint-plugin-astro": "^1.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "sass": "^1.93.2",
    "sass-embedded": "^1.93.2",
    "tailwindcss": "^4.1.16",
    "typescript-eslint": "^8.46.2"
  }
}
